{
  "name": "paytic/omnipay-mobilpay",
  "type": "library",
  "license": "MIT",
  "description": "Mobilpay driver for ThePhpLeague Omnipay multi-payment processing library",
  "keywords": [
    "mobilpay",
    "driver",
    "gateway",
    "merchant",
    "pay",
    "payment"
  ],
  "homepage": "https://github.com/paytic/omnipay-mobilpay",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "Paytic\\Omnipay\\Mobilpay\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Paytic\\Omnipay\\Mobilpay\\Tests\\": "tests/src",
      "Paytic\\Omnipay\\Mobilpay\\Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "require": {
    "php": "^7.4|^8.0",
    "ext-dom": "*",
    "paytic/omnipay-common": "^3.0"
  },
  "suggest": {
    "ext-soap": "*"
  },
  "require-dev": {
    "ext-soap": "*",
    "ext-json": "*",
    "bytic/phpqatools": "^1.0",
    "omnipay/tests": "^3|^4",
    "vlucas/phpdotenv": "^2.3",
    "nyholm/psr7": ">=1",
    "symfony/http-client": "^5.0|^6.0"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
